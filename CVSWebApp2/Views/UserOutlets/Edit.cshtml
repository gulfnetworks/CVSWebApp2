@model CVSWebApp2.Models.UserOutlet

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<h4>UserOutlet</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="UserOutletId" />
            <div class="form-group">
                <label asp-for="OutletId" class="control-label"></label>
                <select asp-for="OutletId" class="form-control" asp-items="ViewBag.OutletId"></select>
                <span asp-validation-for="OutletId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <select asp-for="Id" class="form-control" asp-items="ViewBag.Id"></select>
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
     
                        <input asp-for="DefaultOutlet" /> @Html.DisplayNameFor(model => model.DefaultOutlet)
             
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
